{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "alias": {
          "server": "./src/server",
          "client": "./src/client"
        }
      }
    ],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-arrow-functions",
    [
      "@babel/plugin-proposal-decorators",
      {
        "decoratorsBeforeExport": true
      }
    ]
  ]
}